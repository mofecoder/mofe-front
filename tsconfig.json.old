{
  "compilerOptions": {
    "target": "ES5",
    "experimentalDecorators": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./app/*"
      ],
      "@/*": [
        "./app/*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxt/vue-app"
    ]
  },
  "files": [
    "./vue-shims.d.ts",
    "./app/types/plugins.d.ts"
  ],
  "include": [
    "./app/**/*.ts",
    "./app/**/*.vue",
  ],
  "exclude": [
    "node_modules"
  ]
}
